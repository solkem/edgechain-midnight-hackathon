{
  "version": 2,
  "name": "edgechain-midnight",
  "buildCommand": "cd packages/ui && cp package.vercel.json package.json && npm install && npm run build",
  "outputDirectory": "packages/ui/dist",
  "installCommand": "echo 'Skipping root install - handled in build script'",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
